{
  "name": "URL for KeePass",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Shows the current URL's hostname in the window title. Click the toolbar button or type CTRL+B to activate.",
  "key": "",
  "permissions": ["activeTab"],
  "icons": {
    "128": "img/icon_128.png",
    "64": "img/icon_64.png",
    "32": "img/icon_32.png",
    "16": "img/icon_16.png"
  },
  "browser_action": {
    "default_title": "URL for KeePass"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+B"
      }
    }
  }
}
